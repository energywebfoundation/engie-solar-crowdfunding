{
  "name": "engie-solar-crowdfunding",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint",
    "prepare": "husky install",
    "test:fe": "nx run ew-crowdfunding:test",
    "lint:fe": "nx run ew-crowdfunding:lint",
    "build:fe": "nx run ew-crowdfunding:build --runner=no-cloud",
    "ci:e2e:fe": "nx e2e ew-crowdfunding-e2e --headless"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html}": [
      "yarn format --uncommitted"
    ],
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run test"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.7",
    "@mui/styles": "^5.2.3",
    "@nrwl/next": "13.4.3",
    "core-js": "^3.6.5",
    "hardhat": "^2.8.2",
    "lint-staged": "^12.1.6",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@nrwl/cli": "13.4.3",
    "@nrwl/cypress": "13.4.3",
    "@nrwl/eslint-plugin-nx": "13.4.3",
    "@nrwl/jest": "13.4.3",
    "@nrwl/linter": "13.4.3",
    "@nrwl/node": "^13.4.4",
    "@nrwl/react": "13.4.3",
    "@nrwl/tao": "13.4.3",
    "@nrwl/web": "13.4.3",
    "@nrwl/workspace": "13.4.3",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@typechain/ethers-v5": "^8.0.5",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "~5.3.0",
    "@typescript-eslint/parser": "~5.3.0",
    "babel-jest": "27.2.3",
    "cypress": "^9.1.0",
    "eslint": "8.2.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "prettier": "^2.3.1",
    "react-test-renderer": "17.0.2",
    "sass": "1.43.2",
    "ts-jest": "27.0.5",
    "typescript": "~4.4.3"
  }
}
