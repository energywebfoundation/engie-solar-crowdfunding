{
  "name": "engie-solar-crowdfunding",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint",
    "prepare": "husky install",
    "test:fe": "nx run ew-crowdfunding:test",
    "lint:fe": "nx run ew-crowdfunding:lint",
    "build:fe": "nx run ew-crowdfunding:build --runner=no-cloud && nx run ew-crowdfunding:export --runner=no-cloud",
    "ci:e2e:fe": "nx e2e ew-crowdfunding-e2e --headless"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html}": [
      "yarn format --uncommitted"
    ],
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run test"
    ]
  },
