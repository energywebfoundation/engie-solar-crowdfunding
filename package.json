    "lint-staged": "^12.1.6",
    "luxon": "^2.3.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.24.2",
    "react-redux": "^7.2.6",
