{
  "printWidth": 120,
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "htmlWhitespaceSensitivity": "ignore",
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "all",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "printWidth": 150,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": true,
        "bracketSpacing": false,
        "explicitTypes": "always"
      }
    }
  ]
}
